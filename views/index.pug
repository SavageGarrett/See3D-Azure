      .col-lg-4.col-xl-3.col-md-6
        .single_catagory
          a(href='/index?model_kit=anatomy-kit')
            h4 Anatomy Kit
          img(src='img/thingiverse/Anatomy-Kit.jpg', alt='Intestine 3D Model')
          // <p> <span>50</span> Available position</p>
// model_kits_end
       
