{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "no-unused-vars": "warn",
    "no-prototype-builtins": "warn",
    "no-unused-labels": "warn",
    "no-case-declarations": "warn"
  }
}
